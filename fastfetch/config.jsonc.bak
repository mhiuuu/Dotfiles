{
    "$schema": "https://github.com/fastfetch-cli/fastfetch/raw/dev/doc/json_schema.json",
    "logo": {
        //"source": "$(find \"${XDG_CONFIG_HOME:-$HOME/.config}/fastfetch/pngs/\" -name \"*.png\" | shuf -n 1)",
        "type": "small",
        "padding": {
            "top": 5,
            "left": 1
        }
    },
    "display": {
        "separator": "  ",
        "color": {
            "keys": "blue"
        },
        "size": {
            "ndigits": 0,
            "maxPrefix": "MB"
        },
        "key": {
            "type": "icon"
        },
        "bar": {
            "charElapsed": "=",
            "charTotal": "-",
            "width": 13
        },
        "percent": {
            "type": 2
        }
    },
    "modules": [
        {
            "type": "custom",
            "format": "\u001b[36m    コンピューター"
        },
        "separator",
        "os",
        "kernel",
        "packages",
        "uptime",
        "wm",
        "localip",
        "break",
        {
            "type": "title",
            "key": "  "
        },
        "separator",
        "wifi",
        "memory",
        "swap",
        {
            "type": "disk",
            "folders": "/"
        },
        {
            "type": "battery",
            "key": "Battery"
        },
        {
            "type": "colors",
            "paddingLeft": 10,
            "symbol": "circle"
        }, 
        "break"
    ]
}
